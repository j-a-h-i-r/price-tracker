{
  "name": "price-tracker",
  "version": "1.0.0",
  "description": "Scripts for tracking prices of products",
  "private": true,
  "exports": "./index.ts",
  "type": "module",
  "engines": {
    "node": "20.11.0",
    "npm": "10.2.4"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prestart": "npm run db:prod",
    "start": "node dist/src/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start:dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "start:debug": "tsnd --inspect --respawn --transpile-only src/index.ts",
    "clean": "rimraf dist/",
    "compile": "tsc",
    "copy-files": "copyfiles emails/** dist",
    "build": "npm-run-all clean compile copy-files",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "knex migrate:latest",
    "db:prod": "npm run migrate:prod && npm run seed:prod",
    "migrate:prod": "knex --cwd dist --env production migrate:latest",
    "seed:prod": "knex --cwd dist --env production seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cheerio": "^0.22.30",
    "@types/email-templates": "^8.0.4",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.45",
    "@types/node-cron": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.22.0",
    "jest": "^27.4.5",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.10",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "email-templates": "^8.0.8",
    "fastify": "^3.25.0",
    "fastify-helmet": "^5.3.2",
    "knex": "^3.1.0",
    "nanoid": "^3.1.30",
    "node-cron": "^3.0.0",
    "p-throttle": "^7.0.0",
    "pg": "^8.7.1",
    "pino": "^7.6.0",
    "pino-pretty": "^7.3.0",
    "pug": "^3.0.2"
  }
}
