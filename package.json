{
  "name": "price-tracker",
  "version": "1.0.0",
  "description": "Scripts for tracking prices of products",
  "private": true,
  "exports": "./index.ts",
  "type": "module",
  "engines": {
    "node": "22.x",
    "npm": "10.x"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prestart": "npm run db:prod",
    "start": "node --experimental-strip-types src/index.ts",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "dev": "cross-env NODE_ENV=development node --watch --watch-preserve-output --experimental-strip-types src/index.ts",
    "fakeweb": "cross-env NODE_ENV=development node --watch --watch-preserve-output --experimental-strip-types src/fake-website-server.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "migrate": "cross-env NODE_ENV=development node --experimental-strip-types knex_migrate.ts",
    "seed": "cross-env NODE_ENV=development node --experimental-strip-types knex_seed.ts",
    "db:prod": "npm run migrate:prod && npm run seed:prod",
    "migrate:prod": "cross-env NODE_ENV=production node --experimental-strip-types knex_migrate.ts",
    "seed:prod": "cross-env NODE_ENV=production node --experimental-strip-types knex_seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/email-templates": "^10.0.4",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.14",
    "@types/node-cron": "^3.0.11",
    "@types/qs": "^6.9.18",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.23.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/otel": "^0.7.0",
    "@opentelemetry/host-metrics": "^0.36.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@sindresorhus/merge-streams": "^4.0.0",
    "cacheable": "^1.9.0",
    "cheerio": "^1.0.0-rc.10",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "email-templates": "^12.0.2",
    "fastify": "^5.2.2",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "nanoid": "^5.1.5",
    "node-cron": "^3.0.3",
    "p-throttle": "^7.0.0",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "pino-opentelemetry-transport": "^1.0.1",
    "pino-pretty": "^13.0.0",
    "pug": "^3.0.3",
    "qs": "^6.14.0",
    "undici": "^7.7.0",
    "zod": "^3.24.3"
  }
}
