{
  "name": "price-tracker",
  "version": "1.0.0",
  "description": "Scripts for tracking prices of products",
  "private": true,
  "exports": "./index.ts",
  "type": "module",
  "engines": {
    "node": "20.11.0",
    "npm": "10.2.4"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prestart": "npm run db:prod",
    "start": "node dist/src/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "start:debug": "tsnd --inspect --respawn --transpile-only src/index.ts",
    "clean": "rimraf dist/",
    "compile": "tsc",
    "copy-files": "copyfiles emails/** dist",
    "build": "npm-run-all clean compile copy-files",
    "test": "jest",
    "test:watch": "jest --watch",
    "migrate": "cross-env NODE_OPTIONS='--loader ts-node/esm' knex migrate:latest --esm",
    "db:prod": "npm run migrate:prod && npm run seed:prod",
    "migrate:prod": "knex --cwd dist --env production migrate:latest",
    "seed:prod": "knex --cwd dist --env production seed:run"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/email-templates": "^10.0.4",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^22.13.14",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/helmet": "^13.0.1",
    "@fastify/otel": "^0.7.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@sindresorhus/merge-streams": "^4.0.0",
    "cheerio": "^1.0.0-rc.10",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "email-templates": "^12.0.2",
    "fastify": "^5.2.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "nanoid": "^5.1.5",
    "node-cron": "^3.0.3",
    "p-throttle": "^7.0.0",
    "pg": "^8.14.1",
    "pino": "^9.6.0",
    "pino-opentelemetry-transport": "^1.0.1",
    "pino-pretty": "^13.0.0",
    "pug": "^3.0.3",
    "qs": "^6.14.0",
    "undici": "^7.7.0",
    "zod": "^3.24.3"
  }
}
